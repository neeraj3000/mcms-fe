{
  "expo": {
    "name": "MCMS",
    "slug": "mcms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Food.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "vapidPublicKey": "BDCCnrY4tIYjdFQSO2oTV-0BvkYdoyDnAyWcOfeNvB1H0iY3tVkN8NlYeS7MEMn1_uFeE3OwekQChbwVBal9u3o"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.starlord78.mess"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Food.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.starlord78.mess",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Food.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Food.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1229ddc8-418d-47b2-9d65-9ce5e2586730"
      }
    },
    "owner": "neeraj3000"
  }
}
